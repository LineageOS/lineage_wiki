---
layout: none
search: exclude
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {%- for page in site.pages %}
  {%- unless page.search == "exclude" %}
  {%- unless page.layout == "redirect" %}
  <url>
    <loc>{{site.url}}{{ page.url }}</loc>
  </url>
  {%- endunless %}
  {%- endunless %}
  {%- endfor %}
</urlset>
